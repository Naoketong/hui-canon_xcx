    background-color: #7db3d9;
    /* text-align: center; */
}

.login-title_sub {
    text-align: center;
    margin-top: 10px;
    font-size: 15px;
    color: #606266;
}

.login-content {
    margin-top: 40px;
    /* width: 70%; */
    height: 150px;
    /*border:1px solid #909399;*/
    /*background-color: #fff;*/
    /*border-radius: 10px;*/
    padding: 20px;
}

.login-input {
    background-color: #fff;
    margin-bottom: 20px;
    height: 48px;
    padding-left: 10px;
    color: #606266;
}

.login-button {
    width: 50%;
    text-align: center;
    background-color: #409EFF;
